{
  "name": "@rolete/rolete",
  "version": "1.1.2",
  "description": "Simple library bundler built on Rollup",
  "private": false,
  "bin": "bin/rolete",
  "main": "lib/rolete.js",
  "typings": "lib/rolete.d.ts",
  "scripts": {
    "lint": "eslint lib bin/rolete",
    "test": "echo \"TODO: testing coming soon\" && exit 1",
    "build": "tsc",
    "clean": "tsc --build --clean",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run build",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/6XGate/rolete.git"
  },
  "keywords": [
    "rollup",
    "rolete"
  ],
  "author": "Matthew Holder (https://www.sleepingcats.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/6XGate/rolete/issues"
  },
  "homepage": "https://github.com/6XGate/rolete#readme",
  "engines": {
    "node": ">=12.9.0"
  },
  "files": [
    "bin/*",
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "dependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "colorette": "^1.2.1",
    "esm": "^3.2.25",
    "lodash": "^4.17.20",
    "pkg-dir": "^5.0.0",
    "pretty-bytes": "^5.5.0",
    "pretty-ms": "^7.0.1",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "strip-ansi": "^6.0.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-auto-install": "^2.1.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/babel__core": "^7.1.12",
    "@types/lodash": "^4.14.167",
    "@types/node": "^15.12.1",
    "@types/yargs": "^16.0.3",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^6.0.0",
    "pinst": "^2.1.1",
    "tslib": "^2.1.0",
    "type-fest": "^1.1.3",
    "typescript": "^4.1.3"
  }
}
