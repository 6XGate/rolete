{
  "name": "@rolete/rolete",
  "version": "1.1.2",
  "description": "Simple library bundler built on Rollup",
  "private": false,
  "bin": "bin/rolete",
  "main": "lib/rolete.js",
  "typings": "lib/rolete.d.ts",
  "scripts": {
    "lint": "eslint lib bin/rolete",
    "fix": "eslint --fix lib bin/rolete",
    "test": "echo \"TODO: testing coming soon\" && exit 1",
    "build": "tsc",
    "clean": "tsc --build --clean",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run build",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/6XGate/rolete.git"
  },
  "keywords": [
    "rollup",
    "rolete"
  ],
  "author": "Matthew Holder (https://www.sleepingcats.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/6XGate/rolete/issues"
  },
  "homepage": "https://github.com/6XGate/rolete#readme",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "bin/*",
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "dependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "colorette": "^1.4.0",
    "esm": "^3.2.25",
    "lodash": "^4.17.21",
    "pkg-dir": "^5.0.0",
    "pretty-bytes": "^5.6.0",
    "pretty-ms": "^7.0.1",
    "rollup": "^2.77.2",
    "rollup-plugin-terser": "^7.0.2",
    "strip-ansi": "^7.0.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-auto-install": "^2.2.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/babel__core": "^7.1.19",
    "@types/lodash": "^4.14.182",
    "@types/node": "^14.*",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.1",
    "pinst": "^3.0.0",
    "tslib": "^2.4.0",
    "type-fest": "^2.17.0",
    "typescript": "^4.7.4"
  }
}
